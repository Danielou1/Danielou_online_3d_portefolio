{
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "functions": {
    "server.ts": {
      "runtime": "node:20.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/danielou-portfolio/browser/index.html"
    }
  ]
}