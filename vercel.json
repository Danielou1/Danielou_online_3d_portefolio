{
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/chat.ts"
    },
    {
      "src": "/(.*\..*)",
      "dest": "/dist/danielou-portfolio/browser/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/danielou-portfolio/browser/index.html"
    }
  ]
}